{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "arrow-parens": "off",
        "operator-linebreak": "off",
        "no-case-declarations": "off",
        "require-jsdoc": "off",
        "max-len": ["error", 120]
    },
    "extends": ["eslint:recommended", "google"]
}